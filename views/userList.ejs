<a href="/new_table/add">Ajouter une tâche</a>
<h1>Liste des tâches</h1>
<ul>
    <% for (let i = 0; i<taches.length; i++) { %>
        <li>
            <%= taches[i] %> 
            <a href="/new_table/update/<%= ids[i]%>">Modifier</a>
            <a href="/new_table/delete/<%= ids[i]%>">Effacer</a>
        </li>
    <% } %>
</ul>